     <ITEM SCONAME="OBJECT" CMPTYPE="1" PARDECLTYP="3" TYPTYPE="1" TYPE="TI_OBJECTS_LOADING"/>
     <ITEM SCONAME="RUN" CMPTYPE="1" PARPASSTYP="1" TYPTYPE="1" TYPE="TI_RUN"/>
