    <SOURCE>`
  DATA _process TYPE ygplmt_process.

  _process-process_id = process_id.
  _process-status = ygpl_mt_status=&gt;in_progress.
*  _process-STATUS = ???
  _process-WP_SERVER = WP_SERVER.
  _process-WP_INDEX = WP_INDEX.
  _process-WP_NO = WP_NO.
  _process-WP_PID = WP_PID.
  _process-timestamp = times-start.
  MODIFY ygplmt_process FROM _process.
  ASSERT sy-subrc = 0.</SOURCE>
