  o_engine-&gt;autosave( ).
  s_param = parameters.
  s_param-process_id = o_engine-&gt;process_id.
  o_engine-&gt;messaging-&gt;send_message( archive = abap_true
                                    category = my_categories-task_param
                                     message = s_param ).</SOURCE>
