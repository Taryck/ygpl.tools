     <ITEM SCONAME="RAWDATA" CMPTYPE="1" MTDTYPE="1" PARPASSTYP="1"/>
    </PARAMETERS>
    <SOURCE>`
  DATA s_param TYPE ts_my_process_info.
  DATA s_task  TYPE ts_process_definition.
  CASE category.
    WHEN my_categories-task_param.
      conv_raw2data( EXPORTING rawdata = rawdata
                      CHANGING    data = s_param ).
      s_task = get_task_informations( s_param-process_id ).
      WRITE:/ &apos;New process&apos;, s_param-process_id, s_task-workprocess-server, s_task-batch_info-jobname, s_task-batch_info-jobcount, s_task-batch_info-jobclass.
    when others.
* Unexcepted case
  ENDCASE.</SOURCE>
