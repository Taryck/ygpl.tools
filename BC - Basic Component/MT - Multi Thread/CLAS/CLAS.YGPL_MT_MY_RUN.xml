  DATA run_id TYPE td_process_id.
  DATA o_category TYPE REF TO ygpl_mt_my_categories.

  CREATE OBJECT o_category.
  super-&gt;constructor( thread_count = thread_count
                        max_memory = max_memory
                         bloc_size = bloc_size
                        categories = o_category ).</SOURCE>
