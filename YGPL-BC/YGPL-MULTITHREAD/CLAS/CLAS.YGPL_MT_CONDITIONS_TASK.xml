  <METHODS>
   <ITEM CMPNAME="CHANGES_SAVED" EXPOSURE="2" STATE="1">
    <TEXTS>
     <ITEM LANG="E" TEXT="Mark changes as saved"/>
    </TEXTS>
    <PARAMETERS>
     <ITEM SCONAME="RESULT" CMPTYPE="1" PARDECLTYP="3" TYPTYPE="1" TYPE="TD_BOOLEAN">
      <TEXTS>
       <ITEM LANG="E" TEXT="Conditions have changed"/>
      </TEXTS>
     </ITEM>
    </PARAMETERS>
    <SOURCE>`
  f_confirm_authorized = abap_true.
  CLEAR f_has_changed.
  o_status-&gt;changes_saved( ).
  o_categories-&gt;changes_saved( ).</SOURCE>
   </ITEM>
   <ITEM CMPNAME="COMPARE_PROGRESSION" EXPOSURE="1" STATE="1" MTDFINAL="X">
    <TEXTS>
     <ITEM LANG="E" TEXT="Compare progression"/>
    </TEXTS>
    <PARAMETERS>
     <ITEM SCONAME="FROM" CMPTYPE="1" PARPASSTYP="1" TYPTYPE="1" TYPE="TS_LIMITS"/>
     <ITEM SCONAME="WITH" CMPTYPE="1" PARPASSTYP="1" TYPTYPE="1" TYPE="TS_LIMITS"/>
    </PARAMETERS>
    <SOURCE>`
  simple_compare text-act from-active with-active.
  simple_compare text-tmi from-time-mini with-time-mini.
  simple_compare text-tma from-time-maxi with-time-maxi.
  simple_compare text-pmi from-progress-mini with-progress-mini.
  simple_compare text-pma from-progress-maxi with-progress-maxi.</SOURCE>
   </ITEM>
   <ITEM CMPNAME="COMPARE_WITH" EXPOSURE="2" STATE="1" MTDNEWEXC="X">
    <TEXTS>
     <ITEM LANG="E" TEXT="Compare with an other conditions"/>
    </TEXTS>
    <PARAMETERS>
     <ITEM SCONAME="CONDITIONS" CMPTYPE="1" PARPASSTYP="1" TYPTYPE="1" TYPE="TI_CONDITIONS">
      <TEXTS>
       <ITEM LANG="E" TEXT="Conditions"/>
      </TEXTS>
     </ITEM>
    </PARAMETERS>
    <EXCEPTIONS>
     <ITEM SCONAME="YGPL_MT_CX" LANGU="E">
      <TEXTS>
       <ITEM LANG="E" TEXT="MultyThread : Root exception class"/>
      </TEXTS>
     </ITEM>
    </EXCEPTIONS>
    <SOURCE>`
  DATA i_conditions TYPE ti_task_conditions.
  TRY.
      i_conditions ?= conditions.
      simple_compare text-str me-&gt;stop_requested i_conditions-&gt;stop_requested.
      simple_compare text-mem me-&gt;maximum_memory i_conditions-&gt;maximum_memory.
      complex_compare text-srv me-&gt;servers i_conditions-&gt;servers.
      simple_compare text-jbp me-&gt;job_priority i_conditions-&gt;job_priority.
      simple_compare text-btr me-&gt;batch_rate i_conditions-&gt;batch_rate.
      simple_compare text-mbs me-&gt;maximum_batch_size i_conditions-&gt;maximum_batch_size.
      simple_compare text-mlt me-&gt;message_lookup_time i_conditions-&gt;message_lookup_time.
      IF i_conditions-&gt;progression &lt;&gt; me-&gt;progression.
        MESSAGE i017 WITH text-prg.
        compare_progression( from = me-&gt;progression
                             with = i_conditions-&gt;progression ).
      ENDIF.
*      set_categories( i_conditions-&gt;categories ).
*      set_status( i_conditions-&gt;status ).
    CATCH cx_sy_move_cast_error.
* Ignore this error
  ENDTRY.</SOURCE>
   </ITEM>
   <ITEM CMPNAME="CONSTRUCTOR" EXPOSURE="2" STATE="1" MTDTYPE="2" MTDNEWEXC="X">
    <TEXTS>
     <ITEM LANG="E" TEXT="CONSTRUCTOR"/>
    </TEXTS>
    <EXCEPTIONS>
     <ITEM SCONAME="YGPL_MT_CX" LANGU="E" MTDTYPE="2">
      <TEXTS>
       <ITEM LANG="E" TEXT="MultyThread : Root exception class"/>
      </TEXTS>
     </ITEM>
    </EXCEPTIONS>
    <SOURCE>`
  DATA o_categories TYPE to_categories.
  DATA o_status     TYPE to_status.

  maximum_memory = 0.
*  SERVERS
  job_priority = &apos;C&apos;.
  progression-active = abap_true.
  progression-time-mini = 15.     &quot; Seconds
  progression-progress-mini = 1.  &quot; %
*  progression-progress-maxi = 5.
  batch_rate = 50.                &quot; %
  message_lookup_time = 5.        &quot; Seconds
  o_categories ?= ygpl_mt_factories=&gt;get_categories( ).
  set_categories( o_categories ).
  o_status ?= ygpl_mt_factories=&gt;get_status( ).
  set_status( o_status ).</SOURCE>
   </ITEM>
   <ITEM CMPNAME="HAS_CHANGED" EXPOSURE="2" STATE="1">
    <TEXTS>
     <ITEM LANG="E" TEXT="Do conditions have been modified"/>
    </TEXTS>
    <PARAMETERS>
     <ITEM SCONAME="RESULT" CMPTYPE="1" PARDECLTYP="3" TYPTYPE="1" TYPE="TD_BOOLEAN">
      <TEXTS>
       <ITEM LANG="E" TEXT="Conditions have changed"/>
      </TEXTS>
     </ITEM>
    </PARAMETERS>
    <SOURCE>`
  result = f_has_changed.
  CHECK result IS INITIAL.
  result = o_categories-&gt;has_changed( ).
  CHECK result IS INITIAL.
  result = o_status-&gt;has_changed( ).</SOURCE>
   </ITEM>
   <ITEM CMPNAME="MERGE_WITH" EXPOSURE="2" STATE="1" MTDNEWEXC="X">
    <TEXTS>
     <ITEM LANG="E" TEXT="Merge with an other conditions"/>
    </TEXTS>
    <PARAMETERS>
     <ITEM SCONAME="CONDITIONS" CMPTYPE="1" PARPASSTYP="1" TYPTYPE="1" TYPE="TI_CONDITIONS">
      <TEXTS>
       <ITEM LANG="E" TEXT="Conditions"/>
      </TEXTS>
     </ITEM>
    </PARAMETERS>
    <EXCEPTIONS>
     <ITEM SCONAME="YGPL_MT_CX" LANGU="E">
      <TEXTS>
       <ITEM LANG="E" TEXT="MultyThread : Root exception class"/>
      </TEXTS>
     </ITEM>
    </EXCEPTIONS>
    <SOURCE>`
  DATA i_conditions TYPE ti_task_conditions.
  TRY.
      i_conditions ?= conditions.
      set_stop_requested( i_conditions-&gt;stop_requested ).
      set_maximum_memory( i_conditions-&gt;maximum_memory ).
      set_servers( i_conditions-&gt;servers ).
      set_job_priority( i_conditions-&gt;job_priority ).
      set_progression( i_conditions-&gt;progression ).
      set_batch_rate( i_conditions-&gt;batch_rate ).
      set_maximum_batch_size( i_conditions-&gt;maximum_batch_size ).
      set_categories( i_conditions-&gt;categories ).
      set_status( i_conditions-&gt;status ).
      set_message_lookup_time( i_conditions-&gt;message_lookup_time ).
    CATCH cx_sy_move_cast_error.
* Ignore this error
  ENDTRY.</SOURCE>
