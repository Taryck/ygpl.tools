   <ITEM REFCLSNAME="YGPLIMT_RUNCONDITIONS_4TASK" EXPOSURE="2" STATE="1" RELTYPE="1">
    <COMPONENTS>
     <ITEM MTDNAME="CONFIRM">
      <SOURCE>`
  CHECK f_has_changed  IS NOT INITIAL OR f_confirm_authorized IS NOT INITIAL.
  RAISE EVENT confirmed EXPORTING conditions = me.
  CLEAR f_confirm_authorized.</SOURCE>
     </ITEM>
     <ITEM MTDNAME="SET_BATCH_RATE">
      <SOURCE>`
  CHECK batch_rate &lt;&gt; value.
  CHECK value &gt; 0 AND value &lt;= 100.
  f_has_changed = abap_true.
  batch_rate = value.</SOURCE>
     </ITEM>
     <ITEM MTDNAME="SET_CATEGORIES">
      <SOURCE>`
  ASSERT value IS BOUND.
  CHECK value &lt;&gt; categories.
  f_has_changed = abap_true.
  categories = value.
  o_categories ?= value.</SOURCE>
     </ITEM>
     <ITEM MTDNAME="SET_JOB_PRIORITY">
      <SOURCE>`
  CHECK ygplimt_runconditions_4task~job_priority &lt;&gt; value.
  CHECK value = &apos;C&apos; OR value = &apos;B&apos; OR value = &apos;A&apos;.
  f_has_changed = abap_true.
  ygplimt_job_moni_conditions~job_priority = ygplimt_runconditions_4task~job_priority = value.</SOURCE>
     </ITEM>
     <ITEM MTDNAME="SET_MAXIMUM_BATCH_SIZE">
      <SOURCE>`
  CHECK maximum_batch_size &lt;&gt; value.
  CHECK value &gt; 0.
  f_has_changed = abap_true.
  maximum_batch_size = value.</SOURCE>
     </ITEM>
     <ITEM MTDNAME="SET_MAXIMUM_MEMORY">
      <SOURCE>`
  CHECK maximum_memory &lt;&gt; value.
  f_has_changed = abap_true.
  maximum_memory = value.</SOURCE>
     </ITEM>
     <ITEM MTDNAME="SET_MESSAGE_LOOKUP_TIME">
      <SOURCE>`
  CHECK message_lookup_time &lt;&gt; value.
  CHECK value &gt; 0.
  f_has_changed = abap_true.
  message_lookup_time = value.</SOURCE>
     </ITEM>
     <ITEM MTDNAME="SET_PROGRESSION">
      <SOURCE>`
  CHECK progression &lt;&gt; value.
  CHECK value-progress-maxi &lt;= 100.
  CHECK value-progress-mini &gt; 0.
  CHECK value-time-mini &gt; 0.
  f_has_changed = abap_true.
  progression = value.</SOURCE>
     </ITEM>
     <ITEM MTDNAME="SET_SERVERS">
      <SOURCE>`
  CHECK ygplimt_runconditions_4task~servers &lt;&gt; value.
  f_has_changed = abap_true.
  ygplimt_job_moni_conditions~servers = ygplimt_runconditions_4task~servers = value.</SOURCE>
     </ITEM>
     <ITEM MTDNAME="SET_STATUS">
      <SOURCE>`
  ASSERT value IS BOUND.
  CHECK value &lt;&gt; status.
  f_has_changed = abap_true.
  status = value.
  o_status ?= value.</SOURCE>
     </ITEM>
     <ITEM MTDNAME="SET_STOP_REQUESTED">
      <SOURCE>`
  CHECK ygplimt_runconditions_4task~stop_requested &lt;&gt; value.
  f_has_changed = abap_true.
  ygplimt_runconditions_4task~stop_requested = value.</SOURCE>
     </ITEM>
