    <SOURCE>`
  ygpl_mt_lock_managment=&gt;check_lock_mode( lock_mode ).

  CALL FUNCTION &apos;DEQUEUE_EYGPLMT_INFOS&apos;
    EXPORTING
      mode_ygplmt_infos = lock_mode
      mandt             = sy-mandt
      process_id        = process_id
      category          = category
      x_process_id      = abap_true
      x_category        = abap_true
      _scope            = &apos;2&apos;
